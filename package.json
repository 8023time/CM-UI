		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"sass": "^1.89.2",
		"svelte": "^5.36.13",
		"svelte-check": "^4.3.0",
		"svelte-preprocess": "5.0.0-alpha.1",
		"typescript": "^5.8.3",
		"vite": "^7.0.6"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"prettierrc": "0.0.0-5"
	}
